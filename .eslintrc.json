{
  "env": {
    "commonjs": true,
    "es2021": true,
    "node": true
  },
  "extends": ["prettier", "standard"],
  "plugins": ["prettier"],
  "parserOptions": {
    "ecmaVersion": 12
  },
  "rules": {
    "no-unused-vars": 1,
    "prettier/prettier": [
      2,
      {
        "endOfLine": "auto"
      }
    ],
    "no-prototype-builtins": "off",
    "no-useless-escape": "off",
    "node/handle-callback-err": "off",
    "space-before-function-paren": "off",
    "no-unreachable": "warn"
  }
}
